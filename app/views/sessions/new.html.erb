<h1>ログイン</h1>

<div>
    <%= form_with(url: login_path, scope: :session) do |f| %>
    <div>
      <%= f.label :code, 'ユーザーID' %>
      <%= f.text_field :code %>
    </div>
    <div>
      <%= f.label :password, 'パスワード' %>
      <%= f.password_field :password %>
    </div>
      <%= f.submit 'ログインする' %>
    <% end %>
</div>
